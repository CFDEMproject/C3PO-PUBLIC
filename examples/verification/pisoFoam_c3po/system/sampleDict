FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPointFace;

setFormat raw;

sets
(
    line
    {
        type            face;
        axis            y;
        start           (4 4.0  4);
        end             (4 10 4);
    }
   
);

fields
(
   
    U
);

